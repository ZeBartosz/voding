{
  "name": "vodding",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "build:preview": "bun run build && bun run preview",
    "lint": "bun oxlint .",
    "lint:fix": "bun oxlint . --fix",
    "lint:strict": "bun oxlint . --type-aware --type-check",
    "lint:strict:fix": "bun oxlint . --type-aware --type-check --fix",
    "lint:css": "stylelint \"**/*.{css,scss}\"",
    "lint:css:fix": "stylelint \"**/*.{css,scss}\" --fix",
    "format": "bun oxfmt",
    "format:check": "bun oxfmt --check",
    "check": "bun run format:check && bun run lint:strict && bun run lint:css",
    "fix": "bun run format && bun run lint:fix && bun run lint:strict:fix && bun run lint:css:fix"
  },
  "dependencies": {
    "idb": "^8.0.3",
    "jspdf": "^3.0.4",
    "lucide-react": "^0.561.0",
    "media-chrome": "^4.16.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-player": "^3.4.0",
    "typescript-eslint": "^8.49.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-dom": "^2.3.12",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-react-x": "^2.3.12",
    "globals": "^16.5.0",
    "oxfmt": "^0.20.0",
    "oxlint": "^1.35.0",
    "oxlint-tsgolint": "^0.10.0",
    "stylelint": "^16.26.1",
    "stylelint-config-recess-order": "^7.4.0",
    "stylelint-config-standard": "^39.0.1",
    "typescript": "~5.9.3",
    "vite": "^5.1.0"
  }
}
